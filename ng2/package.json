{
  "name": "taskmanger-frontend",
  "version": "1.0.1",
  "description": "Simple task manager application. Front-end Angular project.",
  "main": "main.js",
  "scripts": {
    "****** CLEAN ******": "",
    "clean": "npm run clean:test && npm run clean:compiled",
    "clean:all": "npm run clean && npm run clean:node_modules",
    "clean:test": "rimraf coverage",
    "clean:public": "rimraf ./../public/*",
    "clean:electron": "rimraf dist && rimraf asar",
    "clean:compiled": "npm run clean:public && npm run clean:electron",
    "clean:node_modules": "rimraf node_modules",
    "****** TEST ******": "",
    "test": "npm run clean:test && npm run test:karma && npm run test:coverage",
    "test:karma": "karma start ./karma/karma.config.js",
    "test:coverage": "npm run test:remap && npm run test:report",
    "test:remap": "remap-istanbul -i ./coverage/coverage.json -o coverage/coverage.json -t json -e node_modules,tests,karma",
    "test:report": "istanbul report",
    "****** ELECTRON BUILD ******": "",
    "tsc:electron": "tsc main.electron.ts --outDir dist && mv dist/main.electron.js dist/main.js",
    "compile:electron": "npm run test && cross-env NODE_ENV=electron webpack --progress --bail --profile",
    "package:electron": "asar pack dist ./asar/app.asar",
    "build:electron": "npm run clean && npm run tsc:electron && npm run compile:electron && npm run package:electron",
    "****** BUILD ******": "",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --progress --bail --profile",
    "build:production": "npm run clean && cross-env NODE_ENV=production-jit webpack --progress --bail --profile",
    "build:production:aot": "npm run clean && cross-env NODE_ENV=production-aot webpack --progress --bail --profile",
    "****** RUNNING ******": "",
    "start": "webpack-dev-server --inline --port 3000 --watch --content-base src/",
    "electron": "electron dist"
  },
  "author": "Daniel Amores <damoresac@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/damoresa/taskmanager.git"
  },
